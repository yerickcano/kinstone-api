{
  "name": "kinstone-api",
  "version": "1.0.0",
  "description": "Kinstone fusion mini-game backend API with Next.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:system": "ts-node test-system.ts",
    "migrate": "ts-node lib/migrations/migrate.ts",
    "seed": "ts-node lib/seeds/seed.ts",
    "setup": "yarn migrate && yarn seed",
    "reset": "yarn migrate && yarn seed"
  },
  "keywords": [
    "kinstone",
    "fusion",
    "game",
    "api",
    "postgres",
    "typescript",
    "nextjs"
